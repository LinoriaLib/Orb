			wall3.Size = Vector3.new(1, 20, 51)
			wall3.Anchored = true
			wall3.BrickColor = BrickColor.new("Really blue")--
			wall3.Transparency = 0.7
			wall3.TopSurface = "Smooth"
			wall3.BottomSurface = "Smooth"
			wall3.Locked = true
			wall3.CFrame = CFrame.new(25, 10, 0)
