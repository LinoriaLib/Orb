	AddCommand('Override cb'..'a  a'..'b','overcb'..'a',
		function(m,s)
			local lol=Instance.new("StringValue")
			lol.Name="CB".."A Attachment"
			lol.Value=[[CB]]..[[A.overrid]]..[[e=true;]]
			lol.Parent = game:GetService("Workspace")
			wait()
			lol:Destroy()
