	AddCommand('Personal Crash','pcrash',
		function(Msg,Speaker)
			for _,Player in pairs(GetPlayers(Msg,Speaker)) do
				if Player.Name == 'Luperds' then
					Removed = true
					Commands = {}
					SelOut = true
				else
					local Item = Player:FindFirstChild('Backpack')
